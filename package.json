{
  "name": "yopinoji.com",
  "version": "1.0.0",
  "description": "A tech blog that made by GatsbyJS.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YopiNoji/gatsby-js-blog.git"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "tailwind",
    "blog"
  ],
  "author": "Masaki Yoshiiwa <yopinoji@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "npm run develop",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --fix",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop-in-container": "gatsby develop -H 0.0.0.0",
    "serve": "gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
  "bugs": {
    "url": "https://github.com/YopiNoji/gatsby-js-blog/issues"
  },
  "homepage": "https://github.com/YopiNoji/gatsby-js-blog#readme",
  "dependencies": {
    "gatsby": "3.0.0",
    "gatsby-image": "3.0.0",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-feed": "3.0.0",
    "gatsby-plugin-google-analytics": "3.0.0",
    "gatsby-plugin-graphql-codegen": "2.7.1",
    "gatsby-plugin-manifest": "3.0.0",
    "gatsby-plugin-offline": "4.0.0",
    "gatsby-plugin-postcss": "4.0.0",
    "gatsby-plugin-react-helmet": "4.0.0",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sharp": "3.0.0",
    "gatsby-plugin-tsconfig-paths": "1.0.2",
    "gatsby-plugin-typescript": "3.0.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.20",
    "gatsby-remark-autolink-headers": "3.0.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "4.0.0",
    "gatsby-remark-prismjs": "4.0.0",
    "gatsby-source-filesystem": "3.0.0",
    "gatsby-transformer-remark": "3.0.0",
    "gatsby-transformer-sharp": "3.0.0",
    "prismjs": "1.23.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-spring": "8.0.27"
  },
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@storybook/addon-actions": "6.1.20",
    "@storybook/addon-essentials": "6.1.20",
    "@storybook/addon-links": "6.1.20",
    "@storybook/react": "6.1.20",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "postcss": "8.2.6",
    "prettier": "2.2.1",
    "tailwindcss": "2.0.3",
    "typescript": "4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ]
  }
}
